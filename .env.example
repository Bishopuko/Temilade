# Shared Infrastructure Configuration
# Copy this file to .env and configure the values

# RabbitMQ Configuration
RABBITMQ_USER=admin
RABBITMQ_PASS=admin

# Database Defaults (can be overridden per service)
USER_DB_NAME=user_service
USER_DB_USER=user
USER_DB_PASS=password

TEMPLATE_DB_NAME=template_service
TEMPLATE_DB_USER=template
TEMPLATE_DB_PASS=password

# Service URLs (used by API Gateway)
USER_SERVICE_URL=http://user_service:5000
TEMPLATE_SERVICE_URL=http://template_service:8081
