# Template Service Configuration
# Copy this file to .env and configure the values

# PHP Environment
APP_ENV=development
APP_DEBUG=true

# Database Connection
DB_HOST=postgres_template
DB_PORT=5432
DB_DATABASE=template_service
DB_USERNAME=template
DB_PASSWORD=password

# Redis Configuration
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=null

# Cache Configuration
CACHE_DRIVER=redis
SESSION_DRIVER=redis

# Logging
LOG_CHANNEL=stack
LOG_LEVEL=debug
